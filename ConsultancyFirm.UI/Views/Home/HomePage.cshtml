@model List<Heading>
@{
    ViewData["Title"] = "Home Page";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="row">
    <div class="col-md-3">

    </div>
    <div class="col-md-9">
        @foreach (var head in Model)
        {
            <div class="card">
                <div class="card-header">
                    <h1>@head.HeadingName</h1>
                </div>
                <div class="card-body">
                    <p>@head.HeadingContent</p>
                </div>

            </div>
        }

    </div>

</div>