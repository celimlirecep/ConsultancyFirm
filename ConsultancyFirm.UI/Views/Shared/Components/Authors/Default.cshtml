@model List<Author>


@foreach (var author in Model)
{
<div class="col-md-6">
    <div class="cards-list">

        <div class="carda ">
            <div class="card_image"> <img src="~/images/defaultAuthor/@author.AuthorImages" /> </div>
            <div class="card_title title-white">
                <p style="background-color:black !important;">@author.AuthorFullName</p>
            </div>
        </div>

    </div>

</div>


}